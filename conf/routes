
GET     /                           controllers.BaseController.index

GET     /addMember                  controllers.MemberController.renderAddMember
POST    /addMember                  controllers.MemberController.addMember
GET     /editMember/:id             controllers.MemberController.renderEditMember(id:Integer)
GET     /editMember/:id             controllers.MemberController.editMember(id:Integer)

GET     /chapters                   controllers.ChapterController.index

GET     /search                     controllers.SearchController.search

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
